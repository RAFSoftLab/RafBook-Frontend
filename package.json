{
  "name": "raf-book",
  "private": true,
  "version": "0.0.0",
  "main": "electron/dist/main.cjs",
  "scripts": {
    "dev": "vite",
    "electron:dev": "concurrently \"vite\" \"tsc -p electron/tsconfig.json --watch\" \"wait-on electron/dist/main.cjs && wait-on http://localhost:5173 && electron electron/dist/main.cjs\"",
    "build": "tsc -b && vite build",
    "electron:build": "vite build && tsc -p electron/tsconfig.json && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/open-sans": "^5.1.0",
    "@fontsource/roboto": "^5.1.0",
    "@giphy/js-fetch-api": "^5.6.0",
    "@giphy/react-components": "^9.8.0",
    "@mui/icons-material": "^6.1.10",
    "@mui/material": "^6.4.0",
    "@reduxjs/toolkit": "^2.4.0",
    "axios": "^1.7.7",
    "electron-store": "^10.0.0",
    "emoji-mart": "^5.6.0",
    "firebase": "^11.0.2",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^11.11.17",
    "lodash.debounce": "^4.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.1",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/cypress": "^0.1.6",
    "@types/fluent-ffmpeg": "^2.1.3",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^7.6.0",
    "cypress": "^14.0.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "electron-updater": "^6.3.9",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "fluent-ffmpeg": "^2.1.3",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^6.0.11",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.raf.book",
    "productName": "Raf Book",
    "files": [
      "dist/**/*",
      "electron/dist/**/*",
      "electron/preload.js"
    ],
    "directories": {
      "output": "release-builds"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
